{
  "name": "tweego-bin",
  "author": "double-a-stories",
  "license": "Apache-2.0",
  "version": "0.1.2",
  "description": "Distribute and install Go binaries via NPM",
  "bin": {
    "tweego": "bin/tweego",
    "storyformats": "bin/storyformats"
  },
  "cpu": [
    "x64",
    "ia32"
  ],
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "scripts": {
    "build": "rm -rf ./dist && esbuild --bundle src/install_tweego.js --outfile=\"./dist/install.js\" --platform=node --target=es6 --minify-whitespace",
    "prepack": "npm run build",
    "postinstall": "node ./dist/install.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/double-a-stories/tweego-bin.git"
  },
  "homepage": "https://github.com/double-a-stories/tweego-bin",
  "devDependencies": {
    "axios": "^0.24.0",
    "esbuild": "^0.12.17",
    "unzipper": "0.10.10"
  }
}